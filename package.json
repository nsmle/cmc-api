  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "license": "MIT",
  "author": {
    "name": "Fiki Pratama",
